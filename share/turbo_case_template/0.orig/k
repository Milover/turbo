FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include		"../turbo_values";

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform $TurbulenceKineticEnergy;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet
    {
        type            freestream;
		freestreamValue	$internalField;
		value			$internalField;
    }

    ".*wall.*"
    {
        type            kqRWallFunction;
		value			$internalField;
    }

    ".*periodic.*"
    {
        type            cyclicAMI;
    }

	topAndBot
	{
		type			empty;
	}
}


